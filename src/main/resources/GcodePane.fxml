<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.robotcontrol.gui.gCodePane.GCodeController">
   <children>
      <HBox prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="267.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <VBox prefHeight="100.0" prefWidth="189.0">
                           <children>
                              <Label fx:id="fileNameLabel" prefHeight="24.0" prefWidth="307.0" text="file name" textAlignment="CENTER">
                                 <font>
                                    <Font size="19.0" />
                                 </font>
                              </Label>
                              <JFXButton onAction="#openFile" prefHeight="28.0" prefWidth="99.0" text="Open file" />
                              <JFXButton onAction="#fileCalc" text="select and calculate" />
                           </children>
                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Label fx:id="action" text="Label" />
                              <ProgressIndicator fx:id="calcIndicator" prefHeight="45.0" prefWidth="24.0" progress="0.0" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <JFXTextArea fx:id="gCodeArea" />
                        <JFXButton onAction="#areaCalc" text="select and calculate" />
                     </children>
                  </VBox>
                  <JFXButton onAction="#go" prefHeight="26.0" prefWidth="84.0" text="Go!" />
               </children>
            </VBox>
            <Pane fx:id="drawingPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="533.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
