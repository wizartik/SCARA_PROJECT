<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.robotcontrol.gui.gCodePane.GCodeController">
   <children>
      <HBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="267.0">
               <children>
                  <VBox prefHeight="93.0" prefWidth="267.0">
                     <children>
                        <Label fx:id="fileNameLabel" prefHeight="24.0" prefWidth="307.0" text="file name" textAlignment="CENTER">
                           <font>
                              <Font size="19.0" />
                           </font>
                        </Label>
                        <JFXButton onAction="#openFile" prefHeight="28.0" prefWidth="99.0" text="Open file" />
                        <JFXButton onAction="#fileCalc" text="select and calculate" />
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <JFXTextArea fx:id="gCodeArea" />
                        <JFXButton onAction="#areaCalc" text="select and calculate" />
                     </children>
                  </VBox>
                  <JFXButton onAction="#go" prefHeight="26.0" prefWidth="84.0" text="Go!" />
               </children>
            </VBox>
            <Pane prefHeight="400.0" prefWidth="336.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
